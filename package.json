{
  "name": "spademo",
  "version": "1.0.0",
  "description": "Demo ",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "watch:sass": "node-sass frontend/sass/main.scss frontend/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass frontend/sass/main.scss frontend/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' frontend/css/style.comp.css -o frontend/css/style.prefix.css",
    "compress:css": "node-sass frontend/css/style.prefix.css frontend/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FigueroaR/SPAdemo.git"
  },
  "author": "R.F.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FigueroaR/SPAdemo/issues"
  },
  "homepage": "https://github.com/FigueroaR/SPAdemo#readme"
}
